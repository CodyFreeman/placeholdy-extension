{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es5", "es2017", "dom"],
    "module": "commonJS",
    "declaration": false,
    "sourceMap": false,
    "removeComments": true,
    "downlevelIteration": true,
    "allowJs": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitReturns": false,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true
  }
}
